<div class="resume-full">
  <h2 class="owner-name">
    <%=resume.escape("poster_name")%>
  </h2>

  <% if (resume.escape("resume_pdf")) { %>
    <embed src="<%=resume.escape('resume_pdf')%>">
  <% } else { %>
    <div class="resume-text">
      <%= resume.escape('text') %>
    </div>
  <% } %>

  <span class=last-updated>
    <%= lastUpdated %>
  </span>
</div>
